{
  "name": "flow-connect",
  "version": "1.0.1-beta",
  "description": "FlowConnect is a highly-customizable library for creating node-based editors, graphs and diagrams.",
  "main": "lib/flow-connect.js",
  "scripts": {
    "test": "npx cypress run",
    "test:chrome": "npx cypress run -b chrome",
    "test:headless": "npx cypress run -b chrome --headless",
    "clean": "shx rm -rf bundles lib lib-esm",
    "build": "npm run clean && tsc && tsc -m es6 --outDir lib-esm && webpack --config=webpack.prod.js",
    "dev": "npx webpack serve --config=webpack.dev.js",
    "docs:dev": "vuepress dev docs",
    "docs:build": "vuepress build docs"
  },
  "keywords": [
    "node editor",
    "node-based editor",
    "HTML5 node editor",
    "node-editor framework",
    "flow-connect",
    "flow connect",
    "canvas library"
  ],
  "author": "Saurabh Bhagat",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.16.7",
    "@babel/preset-env": "^7.16.7",
    "@babel/preset-typescript": "^7.16.7",
    "@types/cypress": "^1.1.3",
    "@types/node": "^14.18.3",
    "@types/offscreencanvas": "^2019.6.4",
    "@vuepress/plugin-search": "^2.0.0-beta.32",
    "chai-arrays": "^2.2.0",
    "cross-blob": "^3.0.0",
    "cypress": "^7.7.0",
    "markdown-it-attrs": "^4.1.3",
    "terser-webpack-plugin": "^5.3.0",
    "ts-loader": "^8.3.0",
    "typedoc": "^0.22.10",
    "typedoc-plugin-markdown": "^3.11.11",
    "typescript": "^4.5.4",
    "vuepress": "^2.0.0-beta.32",
    "webpack": "^5.65.0",
    "webpack-cli": "^4.9.1",
    "webpack-dev-server": "^4.7.2",
    "webpack-merge": "^5.8.0"
  }
}
