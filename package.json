{
  "name": "flow-connect",
  "version": "1.0.3",
  "description": "FlowConnect is a highly-customizable library for creating node-based editors, graphs and diagrams.",
  "main": "dist/cjs/flow-connect.js",
  "types": "dist/cjs/flow-connect.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/saurabh-prosoft/flow-connect.git"
  },
  "scripts": {
    "test": "jest",
    "clean": "npx shx rm -rf dist",
    "prebuild": "npm run clean",
    "build": "npm run build:umd && npm run build:cjs && npm run build:esm",
    "build:umd": "webpack --config=webpack.prod.js",
    "build:cjs": "tsc",
    "build:esm": "tsc -m es6 --outDir dist/esm",
    "dev": "npx webpack serve --config=webpack.dev.js",
    "docs:dev": "vuepress dev docs",
    "docs:build": "vuepress build docs --clean-temp --clean-cache",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "node editor",
    "node-based editor",
    "flow-connect",
    "HTML5 node editor",
    "node-editor framework",
    "flow connect",
    "canvas library",
    "visual programming",
    "visual programming ui library",
    "node ui library"
  ],
  "author": "Saurabh Bhagat",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.17.8",
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-typescript": "^7.16.7",
    "@types/jest": "^27.4.1",
    "@types/node": "^14.18.3",
    "@types/offscreencanvas": "^2019.6.4",
    "@vuepress/plugin-register-components": "^2.0.0-beta.43",
    "@vuepress/plugin-search": "^2.0.0-beta.43",
    "babel-jest": "^27.5.1",
    "cross-blob": "^3.0.0",
    "jest": "^27.5.1",
    "markdown-it-attrs": "^4.1.3",
    "terser-webpack-plugin": "^5.3.0",
    "ts-jest": "^27.1.4",
    "ts-loader": "^8.3.0",
    "ts-node": "^10.7.0",
    "typedoc": "^0.22.10",
    "typedoc-plugin-markdown": "^3.11.11",
    "typescript": "^4.5.4",
    "vuepress": "^2.0.0-beta.43",
    "webpack": "^5.65.0",
    "webpack-cli": "^4.9.1",
    "webpack-dev-server": "^4.7.2",
    "webpack-merge": "^5.8.0"
  }
}
