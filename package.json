{
  "name": "flow-connect",
  "version": "1.0.1",
  "description": "FlowConnect is a highly-customizable library for creating node-based editors, graphs and diagrams.",
  "main": "lib/flow-connect.js",
  "scripts": {
    "test": "npx cypress run -b chrome",
    "test:headless": "npx cypress run -b chrome --headless",
    "clean": "shx rm -rf bundles lib lib-esm",
    "build": "npm run clean && tsc && tsc -m es6 --outDir lib-esm && webpack --config=webpack.prod.js",
    "doc": "npx typedoc --excludePrivate --excludeProtected --readme media/README-docs.md --media media --out docs src/flow-connect.ts",
    "dev": "npx webpack serve --config=webpack.dev.js"
  },
  "keywords": [
    "Node editor",
    "Node-based editor",
    "HTML5 node editor",
    "node-editor framework"
  ],
  "author": "Saurabh Bhagat",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.14.6",
    "@babel/preset-env": "^7.14.7",
    "@babel/preset-typescript": "^7.14.5",
    "@types/cypress": "^1.1.3",
    "@types/node": "^14.17.4",
    "chai-arrays": "^2.2.0",
    "cypress": "^7.6.0",
    "cypress-plugin-snapshots": "^1.4.4",
    "ts-loader": "^8.3.0",
    "typescript": "^4.3.5",
    "uglifyjs-webpack-plugin": "^2.2.0",
    "webpack": "^5.42.0",
    "webpack-cli": "^4.7.2",
    "webpack-dev-server": "^4.5.0",
    "webpack-merge": "^5.8.0"
  },
  "dependencies": {
    "@types/offscreencanvas": "^2019.6.4",
    "typedoc": "^0.21.2"
  }
}
